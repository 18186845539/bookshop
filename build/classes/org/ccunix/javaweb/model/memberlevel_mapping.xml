<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.ccunix.javaweb.model">
	<class name="MemberLevelModel" table="memberlevel">
		<id name="id" column="id" type="int"></id>
		<property name="levelName" column="levelName" type="string"></property>
		<property name="favourable" column="favourable" type="int"></property>
	</class>

</hibernate-mapping>